	"Snippet": {
	"prefix": "snippet",
	"body": [
		"\"${1:description}\": {",
		"\"prefix\": \"${2:prefix}\",",
		"\"body\": [",
		"  \"${3:body}\"",
		"],",
		"\"description\": \"${1:description}\"",
		"},"
	],
	"description": "Snippet"
	}
	
}